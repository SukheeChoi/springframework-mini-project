<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
                 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mycompany.webapp.dao.mybatis.MyDentistsDao">


	<select id="selectByUserid" parameterType="string" resultType="mydentists">
		SELECT * FROM mydentists WHERE userid=#{userid}
	</select>
	
	<insert id="insert" parameterType="mydentists">
		INSERT INTO mydentists VALUES(
			userid=#{userid}
			, denno=#{denno}
		)
	</insert>
	
	<delete id="delete" parameterType="mydentists">
		DELETE FROM mydentists
		WHERE userid=#{userid}
			, denno=#{denno}
	</delete>

</mapper>

